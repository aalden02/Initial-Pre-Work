//Similar to the importance of proper syntax, proper logical flow is important in else if statement.
function orderMyLogic(val) {
  if (val < 5) {
    return "Less than 5";}
    else if (val < 10) {
    return "Less than 10";}
    else {
    return "Greater than or equal to 10";
  }
}

// Change this value to test
orderMyLogic(7);
